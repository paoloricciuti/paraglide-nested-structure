{
	"name": "sveltelab-typescript",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"format": "prettier --plugin-search-dir . --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"test": "playwright test --ui",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.1.0",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.10.8",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unplugin-auto-import": "^0.17.3",
		"vite": "^5.0.11",
		"zimmerframe": "^1.1.0"
	},
	"dependencies": {
		"@inlang/paraglide-js-adapter-vite": "^1.2.0",
		"acorn": "^8.11.3",
		"magic-string": "^0.30.5"
	}
}
